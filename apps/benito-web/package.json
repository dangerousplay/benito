  "keywords": [
    "react",
    "vite",
    "typescript",
    "vitest",
    "tailwind",
    "template",
    "starter",
    "eslint",
    "prettier",
    "husky",
    "commit-lint"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not IE 11"
  ],
  "engines": {
    "node": ">=14",
    "yarn": ">=1.22.5"
  },
  "scripts": {
    "dev": "node_modules/.bin/vite",
